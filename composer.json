{
    "name": "ganodermaki/laravel-small-program",
    "description": "laravel small program library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ganodermaking",
            "email": "505557473@qq.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^6.3@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Ganodermaking\\LaravelSmall\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ganodermaking\\LaravelSmall\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ganodermaking\\LaravelSmall\\SmallServiceProvider"
            ],
            "aliases": {
                "small-program": "Ganodermaking\\LaravelSmall\\SmallFacade"
            }
        }
    }
}
